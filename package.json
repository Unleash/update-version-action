{
  "name": "update-version-action",
  "version": "1.0.0",
  "description": "Github Action for updating Unleash distribution versions in BigQuery",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unleash/update-version-action.git"
  },
  "scripts": {
    "build": "tsc .",
    "lint": "eslint .",
    "test": "jest"
  },
  "keywords": [
    "Github",
    "Action",
    "Unleash",
    "BigQuery",
    "version"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Unleash/update-version-action/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "homepage": "https://github.com/Unleash/update-version-action#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@google-cloud/bigquery": "^7.2.0",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@types/semver": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-plugin-github": "^4.10.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "ts-jest": "^29.1.1",
    "tsc": "^2.0.4",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ]
  }
}
